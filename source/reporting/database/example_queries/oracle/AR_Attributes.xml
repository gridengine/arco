<Query tableName="view_ar_attribute" limit="0" category="Advance Reservation" name="Advance Reservation Attributes" type="simple">
    <view>
        <description/>
        <parameter visible="true"/>
        <sql/>
    </view>
    <field dbName="ar_number" function="VALUE" reportName="ar_number"/>
    <field dbName="owner" function="VALUE" reportName="owner"/>
    <field dbName="submission_time" function="VALUE" reportName="submission_time"/>
    <field dbName="name" function="VALUE" reportName="name"/>
    <field dbName="account" function="VALUE" reportName="account"/>
    <field dbName="start_time" function="VALUE" reportName="start_time"/>
    <field dbName="end_time" function="VALUE" reportName="end_time"/>
    <field dbName="granted_pe" function="VALUE" reportName="granted_pe"/>
    <filter condition="equal" lateBinding="true" active="true" parameter="" name="ar_number"/>
    <sql>SELECT "ar_number", "owner", "submission_time", "name", "account", "start_time", "end_time", 
         "granted_pe" FROM (SELECT ar_number, owner, submission_time, name, account, start_time, end_time,
         granted_pe FROM view_ar_attribute) AS tmp WHERE "ar_number" = '';</sql>
</Query>


