<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Query type="advanced" name="Statistic History" category="Administration">
    <description>Shows the number of rows of each database table</description>
    <view>
        <description/>
        <parameter/>
        <sql/>
        <graphic visible="true" order="0">
            <chart seriesType="row" type="bar3d">
                <xaxis>time_end</xaxis>
                <seriesFromColumns/>
                <seriesFromRow>
                    <label>name</label>
                    <value>num_value</value>
                </seriesFromRow>
            </chart>
        </graphic>
        <pivot/>
        <table/>
    </view>
    <field reportName="time_end" dbName="time_end"/>
    <field reportName="name" dbName="name"/>
    <field reportName="num_value" dbName="num_value"/>
    <sql>select time_end, name, num_value from view_statistic
where variable = 'row_count' and num_value &gt; 10</sql>
</Query>

