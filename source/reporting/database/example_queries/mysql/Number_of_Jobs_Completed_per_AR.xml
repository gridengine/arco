<Query tableName="" category="Job" name="Number of Jobs Completed per AR" type="advanced">
   <description>Daily number of completed jobs per Advance Reservation for the interval of one year.</description>
   <view>
      <description visible="true"/>
      <parameter/>
      <sql visible="true"/>
   </view>
    <sql>SELECT ar_number AS ar_number,
                date_format(time, '%Y-%m-%d') AS day, 
                sum(completed) AS jobs
         FROM view_jobs_completed 
         WHERE time  &gt;  (current_timestamp - interval 1 year)
         AND ar_number > 0
         GROUP BY day, ar_number</sql>
</Query>

