<Query tableName="sge_ar" limit="0" category="Advance Reservation" name="Advance Reservation by User" type="simple">
   <description>Shows all Advance Reservations submitted by the given user.</description>   
    <view>
        <description visible="true"/>
        <parameter visible="true"/>
        <sql/>
    </view>
    <field dbName="ar_number" function="VALUE" reportName="ar_number"/>
    <field dbName="ar_submission_time" function="VALUE" reportName="submission_time"/>
    <filter condition="equal" lateBinding="true" active="true" parameter="" name="ar_owner"/>
    <sql><!--In simple queries sql  is created at run time--></sql>
</Query>

