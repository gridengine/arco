<Query tableName="view_queue_values" limit="0" category="Resource Usage" name="Queue Consumables" type="simple">
    <view>
        <description visible="true"/>
        <parameter visible="true"/>
        <sql visible="true"/>
        <graphic order="2" legendVisible="true">
            <bar>
                <xaxis>time_start</xaxis>
                <xaxis>qname</xaxis>
                <yaxis>available</yaxis>
            </bar>
        </graphic>
        <pivot order="1">
            <row>qname</row>
            <column>used</column>
            <data>variable</data>
        </pivot>
        <table order="0">
            <column>used</column>
            <column>available</column>
        </table>
    </view>
    <field dbName="num_value" function="SUM" reportName="used"/>
    <field dbName="num_config" function="SUM" reportName="available"/>
    <field dbName="qname" function="VALUE" reportName="qname"/>
    <field dbName="variable" function="VALUE" reportName="variable"/>
    <field dbName="time_start" function="VALUE" reportName="time_start"/>
    <filter condition="equal" lateBinding="true" active="true" parameter="slots" name="variable"/>
    <sql>SELECT "used", "available", "qname", "variable", "time_start" FROM(SELECT SUM(num_value) AS "used",SUM(num_config) AS "available",qname,variable,time_start FROM view_queue_values GROUP BY qname,variable,time_start) AS tmp WHERE "variable" = 'slots';</sql>
</Query>

