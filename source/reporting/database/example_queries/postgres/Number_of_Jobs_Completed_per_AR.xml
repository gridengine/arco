<Query tableName="" category="Job" name="Number of Jobs Completed per AR" type="advanced">
   <description>Daily number of completed jobs per Advance Reservation for the interval of one year.</description>
   <view>
      <description visible="true"/>
      <parameter/>
      <sql visible="true"/>
   </view>
    <sql>SELECT 
            ar_number AS ar_number,
            ar_parent AS ar_id,
            date_trunc('day', time) AS day, 
            SUM(completed) AS jobs
         FROM 
            view_jobs_completed
         INNER JOIN sge_ar 
            ON (ar_id = ar_parent)
         WHERE 
            time &gt; (current_timestamp - interval '1 year')
         AND 
            ar_parent &gt; 0
         GROUP BY 
            day, 
            ar_number,
            ar_parent</sql>
</Query>


