<Query tableName="" category="Job" name="Average Job Turnaround Time" type="advanced">
    <view>
        <description visible="true"/>
        <parameter visible="true"/>
        <sql visible="true"/>
    </view>
    <field dbName="day" function="VALUE" reportName="day"/>
    <field dbName=" turnaround_time::time" function="VALUE" reportName=" turnaround_time::time"/>
    <sql>SELECT day, turnaround_time::time 
	     FROM (SELECT date_trunc('day', start_time) AS day, 
		       AVG(turnaround_time) AS turnaround_time 
			   FROM view_job_times 
			   WHERE start_time &gt; (current_timestamp - interval '1 year') 
		 GROUP BY day) AS tmp;
    </sql>
</Query>

