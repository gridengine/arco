if [ "$JAVA_HOME" = "" ]; then
    echo "JAVA_HOME not set"
    exit 1
else
   if [ ! -x "${JAVA_HOME}/bin/java" ]
   then
       echo "JAVA_HOME/bin/java not found, please check JAVA_HOME ($JAVA_HOME)"
       exit 1
   fi
fi

if [ $# -eq 0 ]; then
   EnterPW_oldmods=`stty -g`
   printf "Enter password >> "
   stty -echo
   read PW
   stty "$EnterPW_oldmods"
   printf "\nRetype password >> "
   stty -echo
   read PW1
   stty "$EnterPW_oldmods"
   printf "\n"
   if [ "$PW" != "$PW1" ]; then
     echo "passwords are not equal"
     exit 1
   fi
else
  PW=$*
fi

${JAVA_HOME}/bin/java -cp WEB-INF/lib/reporting.jar com.sun.grid.arco.util.CryptoHandler $PW
