<!DOCTYPE pagetitle SYSTEM "com_sun_web_ui/dtd/pagetitle.dtd">

<pagetitle>
  <pagebuttons>
    <cc name="SaveButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="primary"/>
      <attribute name="dynamic" value="true"/>
    </cc>
    <cc name="SaveAsButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="secondary"/>
      <attribute name="dynamic" value="true"/>
      <attribute name="onClick" value="javascript: return saveAs();"/>
    </cc>
    <cc name="ResetButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="secondary"/>
      <attribute name="dynamic" value="true"/>
    </cc>
    <cc name="RunButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="secondary"/>
    </cc>
    <cc name="ToAdvancedButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
      <!-- See the CCButton class for button types. -->
      <attribute name="type" value="secondary"/>
    </cc>
  </pagebuttons>
</pagetitle>
