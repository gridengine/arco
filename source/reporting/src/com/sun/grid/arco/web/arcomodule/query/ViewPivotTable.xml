<!DOCTYPE table SYSTEM "tags/dtd/table.dtd">
 
<table>
   <actions>
       <cc name="AddViewPivotColumnButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
         <attribute name="dynamic" value="true" />
       </cc>
       <cc name="AddViewPivotRowButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
         <attribute name="dynamic" value="true" />
       </cc>
       <cc name="AddViewPivotDataButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
         <attribute name="dynamic" value="true" />
       </cc>
       <cc name="RemoveViewPivotButton" tagclass="com.sun.web.ui.taglib.html.CCButtonTag">
         <attribute name="dynamic" value="true" />
         <attribute name="disabled" value="true" />
       </cc>
   </actions>
   <column name="ColName" extrahtml="nowrap='nowrap'">
       <cc name="ValueName" tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
            <attribute name="onChange" value="javascript:setDirty()"/>
            <attribute name="dynamic" value="true"/>
         </cc>
   </column>
   <column name="ColType" extrahtml="nowrap='nowrap'">
         <cc name="ValueType" tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
            <attribute name="onChange" value="javascript:setDirty(); changeFormatOptions( this, 'ValueType', 'ValueFormat' );"/>
            <attribute name="dynamic" value="true"/>
         </cc>
   </column>
   <column name="ColFormat" extrahtml="nowrap='nowrap'">
         <cc name="ValueFormat" tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
            <attribute name="onChange" value="javascript:setDirty()"/>
            <attribute name="dynamic" value="true"/>
         </cc>
   </column>
   <column name="ColPivotType" extrahtml="nowrap='nowrap'">
         <cc name="ValuePivotType" tagclass="com.sun.web.ui.taglib.html.CCDropDownMenuTag">
            <attribute name="onChange" value="javascript:setDirty()"/>
            <attribute name="dynamic" value="true"/>
         </cc>
   </column>
</table>
