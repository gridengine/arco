#
#  Password of the database user
#
DBWRITER_USER_PW=@@DB_USER_PW@@

#
#  Name of the database user
#
DBWRITER_USER=@@DB_USER@@

#
#  Name of the ARCo read user.
#  Needed to create synonyms on Oracle
#
READ_USER=@@READ_USER@@

#
#  JDBC URL to database
#
DBWRITER_URL=@@DB_URL@@

#
# Name of the database schema for the objects
#
DB_SCHEMA=@@DB_SCHEMA@@

#
# Tablespace used for storing tables
#
TABLESPACE=@@TABLESPACE@@

#
# Tablespace used for storing indexes
#
TABLESPACE_INDEX=@@TABLESPACE_INDEX@@

#
#  Continous running mode
#  Default value is true
#
DBWRITER_CONTINOUS=true
	  
#
#  Interval in s for continous
#  Default value is 60 seconds
#
DBWRITER_INTERVAL=@@INTERVAL@@

#
# JDBC driver name
#
DBWRITER_DRIVER=@@DB_DRIVER@@

#
# File name of reporting file
#
DBWRITER_REPORTING_FILE=@@REPORTING_FILE@@

#
# File containing calculation rules
#
DBWRITER_CALCULATION_FILE=@@DERIVED_FILE@@


#
#  The dbwriter writes a warning into the log file if the
#  execution of a single statement tooks longer then the
#  DBWRITER_SQL_THRESHOLD. The threshold is specified in
#  seconds. If the threshold is 0, no warning will be
#  written.
#
DBWRITER_SQL_THRESHOLD=0


#
#  Spool directory of the dbwriter
#  log files and pid file is stored in this directory
#
SPOOL_DIR=@@SPOOL_DIR@@

#
# Debug level
# Valid values: WARNING, INFO, CONFIG, FINE, FINER, FINEST, ALL
#
DBWRITER_DEBUG=@@DEBUG_LEVEL@@
